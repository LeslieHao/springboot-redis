# spring 配置
spring:
  mvc:
    view:
      prefix:classpath:/templates/
      suffix: .html
    static-path-pattern: /*
  resources:
    static-locations: classpath:/static/
  redis:
    host: 127.0.0.1
    port: 6379
    password:
    jedis:
      pool:
        max-active: 8
        min-idle: 8
    timeout: 3600s

# 系统配置
m:
  config:
    name: '车库'
    size: '100'

# 日志配置
logging:
  level:
    com.hh.springbootdemo.action: debug
    org.springframework.web: DEBUG
    root: TARCE




