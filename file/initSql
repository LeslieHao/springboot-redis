CREATE DATABASE SPRINGBOOT_SAMPLE;

USE SPRINGBOOT_SAMPLE;

CREATE TABLE TBL_CAR
(
  ID BIGINT NOT NULL AUTO_INCREMENT,
  CREATE_TIME DATETIME DEFAULT CURRENT_TIMESTAMP NOT NULL,
  LAST_UPDATE_TIME DATETIME DEFAULT CURRENT_TIMESTAMP NOT NULL,
  NAME VARCHAR(32) NOT NULL,
  TYPE VARCHAR(32) NOT NULL,
  COMPANY VARCHAR(32) NOT NULL,
  PRIMARY KEY (ID)
)
  ENGINE=INNODB DEFAULT CHARSET=UTF8;

CREATE DATABASE SPRINGBOOT_SAMPLE_SLAVE;

-- -----------------------------
USE SPRINGBOOT_SAMPLE_SLAVE;

CREATE TABLE TBL_ENGINE
(
  ID BIGINT NOT NULL AUTO_INCREMENT,
  CREATE_TIME DATETIME DEFAULT CURRENT_TIMESTAMP NOT NULL,
  LAST_UPDATE_TIME DATETIME DEFAULT CURRENT_TIMESTAMP NOT NULL,
  NAME VARCHAR(32) NOT NULL COMMENT '发动机',
  COMPANY VARCHAR(32) NOT NULL,
  CYLINDER INT NOT NULL COMMENT '发动机缸数',
  PRIMARY KEY (ID)
)
  ENGINE=INNODB DEFAULT CHARSET=UTF8;